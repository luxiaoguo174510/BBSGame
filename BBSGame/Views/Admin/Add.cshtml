@model ModelInfo.GameType

@{
    Layout = null;
}

<link href="~/Content/bootstrap.css" rel="stylesheet" />
@using (Html.BeginForm())
{
    <table class="table table-bordered">
        <tr>
            <td>游戏类别:</td>
            <td><input type="text" name="GName" value="" /></td>
        </tr>
        <tr>
            <td>状态:</td>
            <td>
                <input type="radio" name="GState" value="禁用" />禁用
                <input type="radio" name="GState" value="启用" checked />启用
            </td>
        </tr>
        <tr>
            <td><input type="button" onclick="location.href='/Admin/Index'" class="btn btn-info" value="返回" /></td>
            <td><input type="submit" name="name" class="btn btn-info" value="提交" /></td>
        </tr>
    </table>
}
