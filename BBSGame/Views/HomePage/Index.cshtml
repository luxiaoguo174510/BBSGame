
@{
    ViewBag.Title = "Index";
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>jQuery可关闭图片层叠轮播代码 - 站长素材</title>
    <link href="~/Content/css/Cooldog.css" rel="stylesheet" />
    <link href="~/Content/css/iconfont.css" rel="stylesheet" />
</head>
<body>
    <div class="Cooldog_container">
        <div class="Cooldog_content">
            <ul>
                <li class="p3">
                    <a href="#">
                        <img alt="" src="~/Content/images/1.png">
                    </a>
                </li>
                <li class="p4">
                    <a href="#">
                        <img alt="" src="~/Content/images/2.png">
                    </a>
                </li>
                <li class="p5">
                    <a href="#">
                        <img alt="" src="~/Content/images/3.png">
                    </a>
                </li>
                <li class="p6">
                    <a href="#">
                        <img alt="" src="~/Content/images/4.png">
                    </a>
                </li>
                <li class="p7">
                    <a href="#">
                        <img alt="" src="~/Content/images/5.png">
                    </a>
                </li>
                <li class="p1">
                    <a href="#">
                        <img alt="" src="~/Content/images/6.png">
                    </a>
                </li>
                <li class="p2">
                    <a href="#">
                        <img alt="" src="~/Content/images/7.png">
                    </a>
                </li>
            </ul>
        </div>
        <a class="btn_left" style="display: none;" href="javascript:;">
            <i class="iconfont icon-zuoyoujiantou"></i>
        </a>
        <a class="btn_right" style="display: none;" href="javascript:;">
            <i class="iconfont icon-zuoyoujiantou-copy-copy"></i>
        </a>
        <a class="btn_close" style="display: none;" href="javascript:;">
            <i class="iconfont icon-icon-test"></i>
        </a>
        <div class="buttons clearfix">
            <a href="javascript:;"></a>
            <a href="javascript:;"></a>
            <a class="color" href="javascript:;"></a>
            <a href="javascript:;"></a>
            <a href="javascript:;"></a>
            <a href="javascript:;"></a>
            <a href="javascript:;"></a>
        </div>
    </div>
    <script src="~/Content/js/jquery.min.js"></script>
    <script src="~/Content/js/Cooldog.js"></script>
</body>
</html>
